  "author": {
    "name": "James Vrana",
    "email": "devjimmyboy@gmail.com",
    "url": "https://jimmyboy.dev"
  },
  "description": "Electron app that manages a stream's music, graphics, and other live features.",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript",
    "stream",
    "nodecg",
    "utility"
  ],
  "repository": {
    "url": "https://github.com/jimmyboy-dev/5Head"
  },
  "productName": "5Head",
  "scripts": {
    "dev": "npm-run-all build:electron --parallel dev:* watch:*",
    "dev:vite": "vite",
    "dev:electron": "electron .",
    "watch:electron": "tsc -w -p electron",
    "build": "yarn run build:vite && yarn run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "yarn run build && electron-builder",
    "pack": "yarn run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc"
  },
  "dependencies": {
